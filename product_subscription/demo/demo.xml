            <field name="subscriber" eval="True"/>
            <field name="is_company" eval="False"/>
            <field name="email">oriane@yourcompany.example.com</field>
            <field name="street">Avenue Wagner, 4</field>
            <field name="city">Namur (Loyers)</field>
