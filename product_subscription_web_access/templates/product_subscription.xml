<odoo>
    <template id="assets_frontend" inherit_id="website.assets_frontend"
        name="Subscribe Form frontend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/product_subscription_web_access/static/src/js/product_subscription.js" />
        </xpath>
    </template>

	<template id="subscription_fields" inherit_id="website_product_subscription.subscription_fields" name="Subscription Form Fields Web Access">
		<xpath expr="//span[@id='subscription_presentation_text']" position="after">
            <span id="web_access_presentation"/>
        </xpath>
	</template>

</odoo>
